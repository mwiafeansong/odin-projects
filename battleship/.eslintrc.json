{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": ["error", "windows"],
    "no-unused-vars": "warn",
    "no-plusplus": "off"
    // "prettier/prettier": "error"
    // "func-names": "off",
    // "object-shorthand": "off"
  }
}
